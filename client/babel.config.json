{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": { "version": "3.27", "proposals": true },
        "useBuiltIns": "usage"
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "version": "^7.19.6",
        "helpers": true,
        "regenerator": true
      }
    ]
  ]
}
